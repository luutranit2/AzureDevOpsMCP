# Azure DevOps Configuration
# Copy this file to .env and fill in your actual values

# Azure DevOps Organization URL (e.g., https://dev.azure.com/your-organization)
AZURE_DEVOPS_ORG_URL=

# Personal Access Token for Azure DevOps API access
AZURE_DEVOPS_PAT=

# Default project name
AZURE_DEVOPS_PROJECT=

# Default story type (e.g., "User Story", "Product Backlog Item")
AZURE_DEVOPS_USER_STORY_TYPE=Product Backlog Item

# Optional: API version (defaults to 7.1-preview.1)
AZURE_DEVOPS_API_VERSION=7.1-preview.1
